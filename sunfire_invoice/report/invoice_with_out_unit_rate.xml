				<t t-set="itotal_amt1" t-value="itotal_amt1+tax_data['amount']"/>
				<!--t t-set="product_hsn_amt" t-value="tax_data['amount']"/> -->
				<t t-set="taxname" t-value="tax_data['name']"/>
				</t>
				</t>
				<t t-foreach="tax_per" t-as="val">
